{
    "files.exclude": {
      ".cache/": true,
      ".venv/": true,
      "*.egg-info": true,
      "pip-wheel-metadata/": true,
      "**/__pycache__": true,
      "**/*.pyc": true,
      "**/.ipynb_checkpoints": true,
      "**/tmp/": true,
      "dist/": true,
      "htmlcov/": true,
      "notebooks/*.yml": true,
      "notebooks/files/": true,
      "notebooks/inventory/": true,
      "prof/": true,
      "site/": true,
      "geckodriver.log": true
    },
    "python.defaultInterpreterPath": ".venv/bin/python",
    "pylint.args": ["--rcfile", ".pylint.ini"],
    "editor.formatOnSave": true,
    "cSpell.words": [
      "applescript",
      "builtins",
      "codecov",
      "codehilite",
      "cygstart",
      "cygwin",
      "dataclasses",
      "Graphviz",
      "ipython",
      "launchservices",
      "levelname",
      "mkdocs",
      "noclasses",
      "pipx",
      "pyenv",
      "pync",
      "ruamel",
      "showfspath",
      "signin",
      "signout",
      "slackoff",
      "unmute",
      "Unmuting",
      "venv",
      "verchew"
    ]
  }
